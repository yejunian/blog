{
  "name": "blog",
  "private": true,
  "description": "yejunian's blog",
  "author": "yejunian",
  "homepage": "https://yejunian.github.io/blog/",
  "scripts": {
    "clone-post": "git clone https://github.com/yejunian/blog-post.git src/blog-post",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "yarn develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve -H 0.0.0.0 --prefix-paths",
    "clean": "gatsby clean",
    "deploy": "yarn clean && yarn build && gh-pages --dist public --no-history",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@giscus/react": "^3.0.0",
    "@mdx-js/react": "^2.0.0",
    "classnames": "^2.3.2",
    "gatsby": "^5.13.7",
    "gatsby-plugin-google-gtag": "^5.13.1",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-mdx": "^5.13.1",
    "gatsby-plugin-minify-classnames": "^5.0.0",
    "gatsby-plugin-sass": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-remark-copy-linked-files": "^6.13.2",
    "gatsby-remark-images": "^7.13.2",
    "gatsby-remark-prismjs": "^7.13.2",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark-footnotes": "3.0.0",
    "remark-gfm": "1.0.0",
    "remark-heading-id": "^1.0.1",
    "sass": "~1.77"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.17.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "gh-pages": "^6.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}
