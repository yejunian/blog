{
  "name": "blog",
  "version": "2.3.0-dev",
  "private": true,
  "description": "yejunian's blog",
  "author": "yejunian",
  "homepage": "https://yejunian.github.io/blog/",
  "scripts": {
    "clone-post": "git clone https://github.com/yejunian/blog-post.git src/blog-post",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "yarn develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve -H 0.0.0.0 --prefix-paths",
    "clean": "gatsby clean",
    "deploy": "yarn clean && yarn build && gh-pages --dist public --no-history",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@giscus/react": "^2.2.0",
    "@mdx-js/react": "^2.1.3",
    "classnames": "^2.3.2",
    "gatsby": "^5.12.4",
    "gatsby-plugin-google-gtag": "^5.12.0",
    "gatsby-plugin-image": "^3.12.0",
    "gatsby-plugin-manifest": "^5.12.0",
    "gatsby-plugin-mdx": "^5.12.0",
    "gatsby-plugin-minify-classnames": "^5.0.0",
    "gatsby-plugin-sass": "^6.12.0",
    "gatsby-plugin-sharp": "^5.12.0",
    "gatsby-remark-copy-linked-files": "^6.12.0",
    "gatsby-remark-images": "^7.12.0",
    "gatsby-remark-prismjs": "^7.12.0",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-sharp": "^5.12.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/mdx": "^2.0.2",
    "@types/node": "^20.6.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
